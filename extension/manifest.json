{
    "manifest_version": 3,
    "name": "NewsStitch",
    "version": "1.0",
    "description": "Sends URL to backend and opens website on click",
    "permissions": ["activeTab", "scripting", "tabs"],
    "host_permissions": ["http://localhost:5000/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "radial-tree-graphic-graph-infographic-svgrepo-com.svg",
        "48": "radial-tree-graphic-graph-infographic-svgrepo-com.svg",
        "128": "radial-tree-graphic-graph-infographic-svgrepo-com.svg"
      }
    }
  }
  